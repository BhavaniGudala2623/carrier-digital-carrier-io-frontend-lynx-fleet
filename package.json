{
  "name": "@carrier-io/frontend-lynx-fleet",
  "description": "LYNX Fleet web application",
  "private": true,
  "version": "1.3.0",
  "author": "Carrier Global Corporation",
  "license": "LICENSE",
  "homepage": "https://bitbucket.org/carrier-digital/carrier-io-frontend-lynx-fleet/src/develop/README.md",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/carrier-digital/carrier-io-frontend-lynx-fleet.git"
  },
  "scripts": {
    "architecture": "depcruise --include-only \"^src\" --output-type dot src | dot -T svg > dependencygraph.svg",
    "check-types": "tsc --noEmit",
    "prebuild": "yarn fetch-secrets",
    "build": "webpack --mode=production",
    "prebuild:dev": "yarn fetch-secrets",
    "build:dev": "webpack --mode=development",
    "report": "webpack --profile --json > ~webpack-stats.json && webpack-bundle-analyzer ~webpack-stats.json",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,html}\" webpack.config.ts",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,scss,html}\" webpack.config.ts",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --fix src/",
    "prepare": "husky install",
    "rc": "cicd_scripts/rc.sh",
    "prestart": "yarn fetch-secrets",
    "preserve": "yarn fetch-secrets",
    "start": "webpack serve --mode=development --open",
    "serve": "webpack serve --mode=development",
    "test": "yarn test:unit",
    "test:unit": "jest --passWithNoTests",
    "fetch-secrets": "aws secretsmanager get-secret-value --secret-id carrier-io-frontend-lynx-fleet-dot-env-file --query SecretString --output text --region us-east-1 > .env",
    "vite": "yarn prestart && vite --host --open"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.jpeg",
    "**/*.jpg",
    "**/*.svg",
    "**/*.png",
    "**/*.gif",
    "i18n.ts"
  ],
